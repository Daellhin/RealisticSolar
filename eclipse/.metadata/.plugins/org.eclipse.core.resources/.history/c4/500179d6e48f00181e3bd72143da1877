public class ItemSilver extends Item {
	public ItemSilver() {
		this.setUnlocalizedName("itemSilver");
		this.setTextureName("rs:itemSilver");
		this.setCreativeTab(RealisticSolarCreativeTab.tabRS);
	}

}
